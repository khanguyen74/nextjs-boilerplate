{
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "^react$",
    "^next",
    "^@/components$",
    "^@/components/(.*)$",
    "^@/hooks/(.*)$",
    "^@mui/material$",
    "^@mui/icons-material/(.*)$",
    "^@/utils$",
    "^@/api/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
